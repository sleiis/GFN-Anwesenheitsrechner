
@inject IDialogService DialogService
<MudDialog>
    <DialogContent>
        @MessageString
    </DialogContent>
    <DialogActions>
        <MudFab OnClick="Cancel" Label="Cancel"></MudFab>
        <MudFab Color="Color.Primary" OnClick="Submit" Label="Ok"></MudFab>
    </DialogActions>
</MudDialog>
@code {

    [Parameter]
    public required string MessageString { get; set; }

    [CascadingParameter]
    private IMudDialogInstance? MudDialog { get; set; }
    // The custom text for the dialog

    private void Submit() => MudDialog!.Close(DialogResult.Ok(true));

    private void Cancel() => MudDialog!.Cancel();

}
